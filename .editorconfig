root = true

[*.java,*.jj,*.jjt]
indent_style = space
indent_size = 3
continuation_indent_size = 0
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.yml,*.yaml,*.xml]
indent_style = space
indent_size = 3
continuation_indent_size = 0
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
